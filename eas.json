{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "bash ./scripts/run-create-google-services.sh"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "bash ./scripts/run-create-google-services.sh"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      },
      "prebuildCommand": "bash ./scripts/run-create-google-services.sh"
    }
  },
  "submit": {
    "production": {}
  }
}
